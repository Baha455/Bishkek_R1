<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.bishkekrielt.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_navigation_home_to_itemFragment2"
            app:destination="@id/itemFragment2" />
    </fragment>

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.bishkekrielt.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/itemFragment2"
        android:name="com.example.bishkekrielt.ui.item.ItemFragment"
        android:label="fragment_item"
        tools:layout="@layout/fragment_item" >
        <action
            android:id="@+id/action_itemFragment2_to_navigation_home"
            app:destination="@id/navigation_home" />
        <argument
            android:name="reCatalog"
            app:argType="com.example.bishkekrielt.data.model.Recatalog" />
        <action
            android:id="@+id/action_itemFragment2_to_mapFragment"
            app:destination="@id/mapFragment" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.bishkekrielt.MainActivity"
        android:label="MainActivity" />
    <fragment
        android:id="@+id/mapFragment"
        android:name="com.example.bishkekrielt.ui.item.MapFragment"
        android:label="MapFragment" />
</navigation>